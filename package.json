  "description": "ndom91 portfolio page",
  "license": "MIT",
  "author": "ndom91 <yo@ndo.dev>",
  "homepage": "https://ndo.dev/",
  "bugs": "https://github.com/ndom91/home2021/issues",
  "repository": "https://github.com/ndom91/home2021",
  "keywords": [
    "next.js",
    "tailwindcss",
    "blog",
    "portfolio",
    "2022"
  ],
  "scripts": {
    "dev": "next dev -p 3003",
    "dev:local": "vercel dev --listen 3003",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc",
    "pretty-quick": "pretty-quick --staged"
  },
  "dependencies": {
    "@liveblocks/client": "^0.15.4",
    "@liveblocks/zustand": "^0.15.4",
    "@mapbox/rehype-prism": "^0.8.0",
    "@next/mdx": "^12.1.0",
    "@supabase/supabase-js": "^1.31.1",
    "@supabase/ui": "^0.36.4",
    "date-fns": "2.28.0",
    "next": "^12.1.0",
    "next-mdx-remote": "^4.0.0",
    "react": "^17.0.2",
    "react-addons-text-content": "^0.0.4",
    "react-dom": "^17.0.2",
    "react-query": "^3.34.16",
    "react-scroll-parallax": "^3.0.5",
    "react-spring": "^9.4.4",
    "react-use": "^17.3.2",
    "splitting": "^1.0.6",
    "supabase-comments-extension": "0.0.2",
    "typescript": "^4.6.2",
    "zustand": "^3.7.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@mdx-js/loader": "^2.1.0",
    "@svgr/webpack": "^6.2.1",
    "@tailwindcss/typography": "^0.5.2",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.41",
    "@types/react-dom": "^17.0.14",
    "autoprefixer": "^10.4.4",
    "chrome-aws-lambda": "^10.1.0",
    "eslint": "^8.11.0",
    "eslint-config-next": "^12.1.0",
    "gray-matter": "4.0.3",
    "playwright-core": "^1.20.0",
    "postcss": "^8.4.12",
    "prettier": "^2.6.0",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "pretty-quick": "^3.1.3",
    "reading-time": "^1.5.0",
    "remark": "14.0.2",
    "remark-html": "15.0.1",
    "remark-prism": "^1.3.6",
    "simple-git-hooks": "^2.7.0",
    "tailwindcss": "^3.0.23",
    "vercel": "^24.0.0"
  },
  "packageManager": "pnpm@7.5.1",
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "@babel/runtime",
        "@mdx-js/react",
        "@react-three/fiber"
      ]
    }
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "bracketSpacing": true
  },
  "eslintConfig": {
    "extends": "next",
    "overrides": [
      {
        "files": [
          "**/api/**/*.[jt]s"
        ],
        "rules": {
          "import/no-anonymous-default-export": [
            "warn",
            {
              "allowArrowFunction": true
            }
          ]
        }
      }
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "npm run pretty-quick"
  }
}
