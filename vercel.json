{
  "headers": [
    {
      "source": "/assets/(.*).mp3",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/a/b.js",
      "destination": "https://stats.ndo.dev/js/plausible.outbound-links.js"
    }
  ]
}
